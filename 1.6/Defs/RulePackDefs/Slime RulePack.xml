    <RulePackDef>
        <defName>NamerInitialSettlementSlimePlayer</defName>
        <rulePack>
            <rulesStrings>
                <li>name->[Adjective] [Noun]</li>
                <li>name->The [Noun]</li>
                <li>name->[Noun] [Number]</li>

                <li>Adjective->Eternal</li>
                <li>Adjective->Primal</li>
                <li>Adjective->Sacred</li>
                <li>Adjective->Infinite</li>
                <li>Adjective->First</li>

                <li>Noun->Nucleus</li>
                <li>Noun->Nexus</li>
                <li>Noun->Cradle</li>
                <li>Noun->Wellspring</li>
                <li>Noun->Sanctum</li>
                <li>Noun->Nexus</li>
                <li>Noun->Monolith</li>
                <li>Noun->Core</li>

                <li>Number->Alpha</li>
                <li>Number->Prime</li>
                <li>Number->One</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>NamerFactionSlime</defName>
        <rulePack>
            <rulesStrings>
                <li>name->The [Adjective] [Noun]</li>
                <li>name->[Noun] [GroupType]</li>

                <li>Adjective->Viscous</li>
                <li>Adjective->Glooping</li>
                <li>Adjective->Gelatinous</li>
                <li>Adjective->Translucent</li>

                <li>Noun->Slime</li>
                <li>Noun->Ooze</li>
                <li>Noun->Puddle</li>
                <li>Noun->Jelly</li>

                <li>GroupType->Collective</li>
                <li>GroupType->Union</li>
                <li>GroupType->Swarm</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>NamerSettlementSlime</defName>
        <rulePack>
            <rulesStrings>
                <li>name->[Adjective] [Noun]</li>
                <li>name->[Syllable][Suffix]</li>

                <li>Adjective->Viscous</li>
                <li>Adjective->Gooey</li>
                <li>Adjective->Bubbling</li>
                <li>Adjective->Dripping</li>
                <li>Adjective->Gelatinous</li>
                <li>Adjective->Translucent</li>

                <li>Noun->Basin</li>
                <li>Noun->Pit</li>
                <li>Noun->Reservoir</li>
                <li>Noun->Grotto</li>
                <li>Noun->Incubator</li>
                <li>Noun->Mire</li>

                <li>Syllable->Glo</li>
                <li>Syllable->Plu</li>
                <li>Syllable->Ooz</li>
                <li>Syllable->Miz</li>
                <li>Syllable->Vask</li>

                <li>Suffix->puddle</li>
                <li>Suffix->oze</li>
                <li>Suffix->slush</li>
                <li>Suffix->blob</li>
                <li>Suffix->mire</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

</Defs>