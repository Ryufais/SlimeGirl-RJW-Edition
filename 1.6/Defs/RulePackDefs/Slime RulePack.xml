<Defs>

    <RulePackDef>
        <defName>Slime_NamerUtility</defName>
        <rulePack>
            <rulesStrings>
                <li>Prefix->Lumi</li>
                <li>Prefix->Vela</li>
                <li>Prefix->Sila</li>
                <li>Prefix->Mina</li>
                <li>Prefix->Ooz</li>
                <li>Prefix->Gelia</li>
                <li>Prefix->Vara</li>
                <li>Prefix->Lyra</li>
                <li>Prefix->Sola</li>

                <li>Suffix->via</li>
                <li>Suffix->is</li>
                <li>Suffix->belle</li>
                <li>Suffix->ra</li>
                <li>Suffix->mira</li>
                <li>Suffix->nne</li>
                <li>Suffix->sia</li>
                <li>Suffix->th</li>

                <li>Designation->Alpha</li>
                <li>Designation->IX</li>
                <li>Designation->Seven</li>
                <li>Designation->Venus</li>
                <li>Designation->Prime</li>
                <li>Designation->Zero</li>

                <li>SoftWord->Visca</li>
                <li>SoftWord->Melisma</li>
                <li>SoftWord->Aura</li>
                <li>SoftWord->Sliss</li>
                <li>SoftWord->Gliss</li>
                <li>SoftWord->Vitreous</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>Slime_LastNameUtility</defName>
        <rulePack>
            <rulesStrings>
                <li>ProjectType->Azure</li>
                <li>ProjectType->Cyan</li>
                <li>ProjectType->Nebula</li>
                <li>ProjectType->Void</li>
                <li>ProjectType->Deep</li>
                <li>ProjectType->Ion</li>
                <li>ProjectType->Pulse</li>
                <li>ProjectType->Ghost</li>

                <li>ProjectUnit->Protocol</li>
                <li>ProjectUnit->Strain</li>
                <li>ProjectUnit->Subject</li>
                <li>ProjectUnit->Nucleus</li>
                <li>ProjectUnit->Synthesis</li>
                <li>ProjectUnit->Entity</li>
                <li>ProjectUnit->Array</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>NamerPawnSlimeFemale</defName>
        <include>
            <li>Slime_NamerUtility</li>
            <li>Slime_LastNameUtility</li>
        </include>
        <rulePack>
            <rulesStrings>
                <li>r_name(p=3)->[FirstPart] [LastPart]</li>
                <li>r_name(p=1)->[FirstPart]</li>
                
                <li>FirstPart->[Prefix][Suffix]</li>
                <li>FirstPart->[SoftWord]</li>

                <li>LastPart->[ProjectType] [ProjectUnit]</li>
                <li>LastPart->[ProjectUnit] [Designation]</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

